{
  "name": "property-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development TURBOPACK=0 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint \"src/**/*.{css,scss}\" \"styles/**/*.css\" --cache --cache-location node_modules/.cache/stylelint",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Test suite removed.\"",
    "test:a11y": "echo \"Accessibility tests removed.\"",
    "test:visual": "echo \"Visual regression tests removed.\"",
    "test:unit": "echo \"Unit tests removed.\"",
    "docs:check": "npx tsx scripts/doc-monitoring-system.ts check",
    "docs:update": "npx tsx scripts/doc-monitoring-system.ts update",
    "docs:validate": "npx tsx scripts/doc-monitoring-system.ts validate",
    "sync:owners": "npx tsx scripts/sync-owners-incremental.ts --hours 24",
    "db:push-indexes": "npx tsx scripts/push-migration.ts supabase/migrations/20250829190000_owner_indexes.sql",
    "db:push-perf": "npx tsx scripts/push-migration.ts supabase/migrations/20250913_performance_indexes.sql",
    "docs:watch": "nodemon --watch src --watch supabase --ext ts,tsx,sql --exec \"npm run docs:check\"",
    "db:schema": "npx supabase@latest db dump --local --file docs/database/current_schema.sql",
    "db:types": "npx supabase@latest gen types typescript --local > src/types/database.ts",
    "db:docs": "npm run db:schema && npm run db:types && npm run docs:update",
    "db:diff:linked": "npx supabase@latest db diff --linked --schema public",
    "db:push:linked": "npx supabase@latest db push --linked",
    "db:reset:linked": "npx supabase@latest db reset --linked",
    "types:local": "npx supabase@latest gen types typescript --local --schema public > src/types/database.ts",
    "types:remote": "npx supabase@latest gen types typescript --linked --schema public > src/types/database.remote.ts",
    "types:prod": "npx supabase@latest gen types typescript --project-id $SUPABASE_PROJECT_REF_PRODUCTION --schema public > src/types/database.remote.ts",
    "types:diff": "git --no-pager diff --no-index -- src/types/database.ts src/types/database.remote.ts || true",
    "sync:bank-accounts": "node scripts/sync-bank-accounts.js",
    "audit": "npm audit --audit-level=high",
    "audit:prod": "npm audit --omit=dev --audit-level=high",
    "audit:fix": "npm audit fix",
    "db:seed": "npx tsx scripts/seed.ts",
    "db:seed-gl": "npx tsx scripts/seed_gl_settings.ts",
    "db:reset": "RESET_CONFIRM=YES npx tsx scripts/reset-db.ts",
    "dev:all": "concurrently -n next,docs -c green,cyan \"npm run dev\" \"npm run docs:watch\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "ci:env-check": "tsx src/env/server.ts && tsx src/env/client.ts",
    "test:buildium:smoke": "echo \"Buildium smoke tests removed.\"",
    "test:buildium:webhook": "echo \"Buildium webhook smoke tests removed.\"",
    "cron:recurring": "tsx scripts/cron/recurring.ts",
    "cron:late-fees": "tsx scripts/cron/late-fees.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@headlessui/react": "^2.2.7",
    "@hookform/resolvers": "^5.2.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.55.0",
    "@opentelemetry/instrumentation-fetch": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.54.0",
    "@opentelemetry/instrumentation-pg": "^0.54.0",
    "@opentelemetry/instrumentation-pino": "^0.52.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^8.55.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/pg": "^8.15.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "playwright": "^1.56.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.2",
    "resend": "^6.3.0",
    "sharp": "^0.33.3",
    "sonner": "^2.0.3",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.1.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "husky": "^9",
    "jsdom": "^24.1.3",
    "lint-staged": "^15",
    "markdownlint": "^0.38.0",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{js,jsx,ts,tsx,md,mdx,json,yaml,yml}": [
      "prettier --write"
    ]
  }
}
